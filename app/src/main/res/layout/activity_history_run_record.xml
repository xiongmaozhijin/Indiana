<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.liangge.indiana.ui.Inner.HistoryRunRecordActivity">

    <com.example.liangge.indiana.ui.widget.CommTitleBar xmlns:title="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        title:titlebar_title="@string/activity_history_titlebar_title">

    </com.example.liangge.indiana.ui.widget.CommTitleBar>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            android:id="@+id/net_hint_wrapper"
            layout="@layout/layout_not_network" />

        <FrameLayout
            android:id="@+id/all_content_wrapper"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@color/bg_1">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingLeft="6dp"
                android:paddingRight="6dp"
                android:paddingTop="4dp">


                <com.example.liangge.indiana.ui.widget.ExListViewScrollDone
                    android:id="@+id/listview"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:divider="@color/white"
                    android:dividerHeight="3dp" />

            </LinearLayout>


        </FrameLayout>

    </FrameLayout>


</LinearLayout>
