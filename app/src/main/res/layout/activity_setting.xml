<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/bg_1"
    tools:context="com.example.liangge.indiana.ui.user.SettingActivity">

    <com.example.liangge.indiana.ui.widget.CommTitleBar
        xmlns:title="http://schemas.android.com/apk/res-auto"
        title:titlebar_title="@string/activity_setting_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <LinearLayout
        android:layout_marginTop="10dp"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <RelativeLayout
            android:id="@+id/user_layout_coupon"
            android:clickable="true"
            android:onClick="onBtnCommQaq"
            android:background="@android:color/white"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/setting_item_height"
            android:layout_below="@+id/user_layout_5">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10.0dip"
                android:text="@string/setting_common_question"
                android:textSize="17.0sp" />

            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toLeftOf="@+id/arrow_right_2" />

            <ImageView
                android:id="@+id/arrow_right_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/setting_item_margin_right"
                android:contentDescription="@string/desc_common_image"
                android:src="@drawable/ic_more" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentBottom="true"
                android:background="@color/common_line" />
        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/setting_check_update"
            android:clickable="true"
            android:onClick="onBtnCheckUpdate"
            android:background="@android:color/white"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/setting_item_height"
            android:layout_below="@+id/user_layout_5">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10.0dip"
                android:text="@string/setting_check_update"
                android:textSize="17.0sp" />

            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toLeftOf="@+id/arrow_right_3" />

            <ImageView
                android:id="@+id/arrow_right_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/setting_item_margin_right"
                android:contentDescription="@string/desc_common_image"
                android:src="@drawable/ic_more" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentBottom="true"
                android:background="@color/common_line" />
        </RelativeLayout>





        <RelativeLayout
            android:id="@+id/setting_about_indiana"
            android:clickable="true"
            android:onClick="onBtnAboutIndiana"
            android:background="@android:color/white"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/setting_item_height"
            android:layout_below="@+id/user_layout_5">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10.0dip"
                android:text="@string/setting_about_indiana"
                android:textSize="17.0sp" />

            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toLeftOf="@+id/arrow_right_4" />

            <ImageView
                android:id="@+id/arrow_right_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/setting_item_margin_right"
                android:contentDescription="@string/desc_common_image"
                android:src="@drawable/ic_more" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentBottom="true"
                android:background="@color/common_line" />
        </RelativeLayout>




        <RelativeLayout
            android:id="@+id/setting_clear_cache"
            android:clickable="true"
            android:onClick="onBtnClearCache"
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:background="@android:color/white"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/setting_item_height"
            android:layout_below="@+id/user_layout_5">


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10.0dip"
                android:text="@string/setting_clear_cache"
                android:textSize="17.0sp" />

            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toLeftOf="@+id/arrow_right_5" />

            <ImageView
                android:id="@+id/arrow_right_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/setting_item_margin_right"
                android:contentDescription="@string/desc_common_image"
                android:src="@drawable/ic_more" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentBottom="true"
                android:background="@color/common_line" />
        </RelativeLayout>




        <RelativeLayout
            android:id="@+id/setting_exit"
            android:clickable="true"
            android:onClick="onBtnExitLog"
            android:background="@android:color/white"
            android:layout_width="fill_parent"
            android:layout_height="@dimen/setting_item_height"
            android:layout_below="@+id/user_layout_5">


            <TextView
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10.0dip"
                android:text="@string/setting_exitlog"
                android:textSize="17.0sp" />

            <TextView
                android:text=""
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="10.0dip"
                android:layout_toLeftOf="@+id/arrow_right_6" />

            <ImageView
                android:id="@+id/arrow_right_6"
                android:visibility="invisible"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="@dimen/setting_item_margin_right"
                android:contentDescription="@string/desc_common_image"
                android:src="@drawable/ic_more" />

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="1.0px"
                android:layout_alignParentBottom="true"
                android:background="@color/common_line" />
        </RelativeLayout>







    </LinearLayout>




</LinearLayout>
